#include "main.h"
#include <stdio.h>

/**
 * print_array - prints n elements of an array of integers
 * @a: argument to function
 * @n: argument to function
 * Return: void
 */

void print_array(int *a, int n)
{
	int e;

	for (e = 0; e < n; e++)
	{
		printf("%d", a[e]);
		if (e == n - 1)
		{
			printf(", ");
		}
		else
		{
			printf("\n");
		}
	}
}
